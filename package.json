{
  "name": "ember-cli-code-coverage",
  "version": "1.0.0-beta.9",
  "description": "Code coverage for ember projects using Istanbul",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "Katie Gengler",
  "contributors": [
    "Matthew Dahl (https://github.com/sandersky)",
    "Robert Jackson (https://github.com/rwjblue)",
    "Robert Wagner (https://github.com/rwwagner90)"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/kategengler/ember-cli-code-coverage",
  "scripts": {
    "browser-test": "COVERAGE=true ember test",
    "changelog": "lerna-changelog",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "node-test": "mocha ./test/{integraion,unit}/**/*-test.js --require @babel/polyfill --require @babel/register",
    "test": "npm run-script lint:js && npm run-script node-test"
  },
  "dependencies": {
    "@babel/register": "^7.6.0",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "body-parser": "^1.19.0",
    "co": "^4.6.0",
    "ember-cli-babel": "^7.17.2",
    "ember-cli-htmlbars": "^4.2.2",
    "ember-cli-version-checker": "^3.1.3",
    "extend": "^3.0.2",
    "fs-extra": "^8.1.0",
    "istanbul-api": "^2.1.6",
    "lodash.concat": "^4.5.0",
    "node-dir": "^0.1.17",
    "rsvp": "^4.8.5",
    "walk-sync": "^2.0.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.2.0",
    "chai-files": "^1.4.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.16.2",
    "ember-cli-addon-tests": "^0.11.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-exam": "1.0.0",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.16.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.4.0",
    "eslint-plugin-ember": "^7.7.2",
    "eslint-plugin-node": "^11.0.0",
    "fixturify": "^0.3.4",
    "glob": "^7.1.3",
    "lerna-changelog": "^0.7.0",
    "loader.js": "^4.7.0",
    "mocha": "^5.2.0",
    "qunit-dom": "^1.0.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.2.2"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "ember-cli-typescript"
  },
  "changelog": {
    "repo": "kategengler/ember-cli-code-coverage",
    "labels": {
      "breaking": ":boom: Breaking Change",
      "enhancement": ":rocket: Enhancement",
      "bug": ":bug: Bug Fix",
      "documentation": ":memo: Documentation",
      "internal": ":house: Internal"
    }
  }
}
